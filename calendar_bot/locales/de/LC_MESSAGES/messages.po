msgid "*Appointment*\n"
msgstr "*Termin*\n"

#: calendar_bot/calendar_bot.py:61
msgid "Eventsummary"
msgstr "Veranstaltungsbezeichnung"

#: calendar_bot/calendar_bot.py:63
msgid "Description"
msgstr "Beschreibung"

#: calendar_bot/calendar_bot.py:65
msgid "Begin"
msgstr "Start"

#: calendar_bot/calendar_bot.py:67 calendar_bot/calendar_bot.py:73
msgid "on"
msgstr "am"

#: calendar_bot/calendar_bot.py:71
msgid "End"
msgstr "Ende"

#: calendar_bot/calendar_bot.py:78
msgid "Location"
msgstr "Ort"

#: calendar_bot/calendar_bot.py:140
msgid "I have got new events for you"
msgstr "Ich habe neue Veranstaltungen für dich"

#: calendar_bot/calendar_bot.py:146
msgid "No new events available"
msgstr "Keine neuen Events verfügbar"

#: calendar_bot/calendar_bot.py:168
msgid "Attention! The following event is coming up"
msgstr "Achtung! Die folgende Veranstaltung steht kurz bevor"

#: calendar_bot/calendar_bot.py:170
msgid "Attention! The following events are coming up"
msgstr "Achtung! Die folgenden Veranstaltungen stehen kurz bevor"

#: calendar_bot/calendar_bot.py:203
msgid "You were already subscribed to the event notification system"
msgstr "Du hast die automatische Kalenderbenachrichtigung bereits abonniert"

#: calendar_bot/calendar_bot.py:207
msgid "You weren't subscribed to the event notification system"
msgstr "Du hattest die automatische Kalenderbenachrichtigung nicht abonniert"

#: calendar_bot/calendar_bot.py:220
msgid "You will not get notified, when a new event has been added, anymore"
msgstr ""
"Du wirst nun nicht mehr benachrichtigt, wenn eine neuer Kalendereintrag "
"hinzukommt"

#: calendar_bot/calendar_bot.py:223
msgid "You wil get notified, when a new event has been added."
msgstr "Du wirst nun benachrichtigt, wenn neue Kalendereinträge hinzukommen"

#: calendar_bot/calendar_bot.py:231
msgid "You can state the following commands:\n"
msgstr "Du kannst einen der folgenden Kommandos absetzen:\n"

#: calendar_bot/calendar_bot.py:232
msgid ""
"\t*{appointments}* gives you all the upcoming events in a list sorted "
"chronologically\n"
msgstr ""
"\t*{appointments}* gibt dir alle anstehenden Veranstaltungen in "
"chronologischer Reihenfolge\n"

#: calendar_bot/calendar_bot.py:233 calendar_bot/calendar_bot.py:245
msgid "appointments"
msgstr "termine"

#: calendar_bot/calendar_bot.py:234
msgid "\t*{sub}* subscribes yourself to the notification system. I will notify "
"you, when a new event has been added and when an event is due\n"
msgstr "\t*{sub}* aktiviert die Benachrichtigungsfunktion. Ich werde dich dann "
"benachrichtigen, wenn eine neue Veranstaltung erstellt wurde oder wenn eine Veranstaltung kurz bevor steht\n"

#: calendar_bot/calendar_bot.py:235 calendar_bot/calendar_bot.py:246
msgid "sub"
msgstr "abo"

#: calendar_bot/calendar_bot.py:236
msgid "\t*{unsub}* unsubscribes you from the notification system\n"
msgstr "\t*{unsub}* deabonniert die automatische Kalenderbenachrichtigung\n"

#: calendar_bot/calendar_bot.py:236 calendar_bot/calendar_bot.py:247
msgid "unsub"
msgstr "deabo"

#: calendar_bot/calendar_bot.py:237
msgid "\t*{help}* prints this helping text\n"
msgstr "\t*{help}* zeigt dir diesen hilfreichen Text an\n"

#: calendar_bot/calendar_bot.py:237 calendar_bot/calendar_bot.py:248
msgid "help"
msgstr "hilfe"

#: calendar_bot/calendar_bot.py:249
msgid "start"
msgstr "start"

